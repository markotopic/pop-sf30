<Window x:Class="pop_sf30_2016.UI.PrikazEntiteta.TipNamestajaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pop_sf30_2016.UI.PrikazEntiteta"
        mc:Ignorable="d"
        Title="TipNamestajaWindow" Height="500" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="177*"/>
            <RowDefinition Height="58*"/>
        </Grid.RowDefinitions>
        <DataGrid Name="dgTipNamestaja" Grid.Row="0" FontSize="15" CanUserAddRows="False" AutoGeneratingColumn="dgTipNamestaja_AutoGeneratingColumn"/>
        
        <Button Grid.Row="1" Content="Dodaj" FontSize="10" Margin="25,35,613,37" Click="Dodaj_Click"/>
        <Button Grid.Row="1" Content="Izmeni" FontSize="10" Margin="105,35,527,37" Click="Izmeni_Click"/>
        <Button Grid.Row="1" Content="Obrisi" FontSize="10" Margin="181,35,454,37" RenderTransformOrigin="1.537,0.396" Click="Obrisi_Click"/>
        <Button x:Name="Izlaz" Grid.Row="1" Content="Izlaz" FontSize="10" Margin="254,35,374,37" Click="Izlaz_Click"/>
    </Grid>
</Window>
